  max-width: 1200px;
  margin: 0 auto;
  padding: 0 24px;
  width: 100%;
}

/* ============================================
   HEADER (Glass Sticky)
   ============================================ */
#site-header {
  position: sticky;
  top: 0;
  z-index: 1000;
  backdrop-filter: blur(18px);
  -webkit-backdrop-filter: blur(18px);
  background: rgba(0, 0, 0, 0.6);
  border-bottom: 1px solid rgba(255, 255, 255, 0.08);
  transition: background-color var(--transition-base), border-color var(--transition-base);
  will-change: background-color, border-color;
}

/* –£–ª—É—á—à–µ–Ω–Ω—ã–π —Ñ–æ–Ω –ø—Ä–∏ –ø—Ä–æ–∫—Ä—É—Ç–∫–µ */
#site-header.scrolled {
  background: rgba(0, 0, 0, 0.75);
  border-bottom-color: rgba(255, 255, 255, 0.12);
}

.nav-wrapper {
  max-width: 1200px;
  margin: 0 auto;
  padding: 16px 24px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 32px;
}

.logo {
  font-size: 20px;
  font-weight: 600;
  color: var(--text-primary);
  letter-spacing: -0.5px;
  white-space: nowrap;
}

.nav-desktop {
  display: flex;
  align-items: center;
  gap: 8px;
  flex: 1;
  justify-content: center;
}

.nav-link {
  padding: 8px 16px;
  color: var(--text-secondary);
  text-decoration: none;
  font-size: 15px;
  font-weight: 500;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
  position: relative;
}

.nav-link:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.nav-link.active {
  color: var(--text-primary);
}

/* Services Nav Dropdown */
.services-nav {
  position: relative;
}

.services-nav-toggle {
  display: flex;
  align-items: center;
  gap: 4px;
  padding: 8px 16px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  font-size: 15px;
  font-weight: 500;
  font-family: var(--font-family);
  cursor: pointer;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.services-nav-toggle:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.nav-arrow {
  font-size: 12px;
  opacity: 0.7;
}

.services-nav-dropdown {
  position: absolute;
  top: calc(100% + 8px);
  left: 0;
  min-width: 220px;
  background: var(--bg-card);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-md);
  box-shadow: var(--shadow-lg);
  padding: 8px;
  opacity: 0;
  visibility: hidden;
  transform: translateY(-8px);
  transition: all var(--transition-base);
  z-index: 1001;
}

.services-nav.active .services-nav-dropdown,
.services-nav:hover .services-nav-dropdown {
  opacity: 1;
  visibility: visible;
  transform: translateY(0);
}

.services-nav-option {
  display: block;
  padding: 10px 16px;
  color: var(--text-secondary);
  text-decoration: none;
  font-size: 14px;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.services-nav-option:hover {
  color: var(--text-primary);
  background: var(--bg-glass-strong);
}

/* Lang Dropdown */
.lang-desktop {
  position: relative;
}

.lang-toggle {
  display: flex;
  align-items: center;
  gap: 4px;
  padding: 8px 16px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  font-size: 15px;
  font-weight: 500;
  font-family: var(--font-family);
  cursor: pointer;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.lang-toggle:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.lang-dropdown {
  position: absolute;
  top: calc(100% + 8px);
  right: 0;
  min-width: 100px;
  background: var(--bg-card);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-md);
  box-shadow: var(--shadow-lg);
  padding: 8px;
  opacity: 0;
  visibility: hidden;
  transform: translateY(-8px);
  transition: all var(--transition-base);
  z-index: 1001;
}

.lang-desktop:hover .lang-dropdown {
  opacity: 1;
  visibility: visible;
  transform: translateY(0);
}

.lang-option {
  display: block;
  width: 100%;
  padding: 10px 16px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  text-align: left;
  font-size: 14px;
  font-family: var(--font-family);
  cursor: pointer;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.lang-option:hover {
  color: var(--text-primary);
  background: var(--bg-glass-strong);
}

/* Mobile Menu Toggle */
.mobile-menu-toggle {
  display: none;
  background: transparent;
  border: none;
  color: var(--text-primary);
  font-size: 24px;
  cursor: pointer;
  padding: 8px;
}

.burger-icon {
  display: block;
}

/* Mobile Menu */
.mobile-menu {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.95);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
  z-index: 2000;
  opacity: 0;
  visibility: hidden;
  transition: all var(--transition-base);
}

.mobile-menu.active {
  opacity: 1;
  visibility: visible;
}

.mobile-menu-content {
  padding: 24px;
  height: 100%;
  overflow-y: auto;
}

.mobile-menu-close {
  position: absolute;
  top: 16px;
  right: 16px;
  background: transparent;
  border: none;
  color: var(--text-primary);
  font-size: 32px;
  cursor: pointer;
  padding: 8px;
  line-height: 1;
}

.mobile-nav {
  display: flex;
  flex-direction: column;
  gap: 8px;
  margin-top: 48px;
}

.mobile-nav-link {
  padding: 16px;
  color: var(--text-secondary);
  text-decoration: none;
  font-size: 18px;
  font-weight: 500;
  border-radius: var(--radius-md);
  transition: all var(--transition-fast);
}

.mobile-nav-link:hover,
.mobile-nav-link.active {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.mobile-lang {
  margin-top: 32px;
}

.mobile-lang-toggle {
  width: 100%;
  padding: 16px;
  background: var(--bg-glass);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-md);
  color: var(--text-primary);
  font-size: 16px;
  font-family: var(--font-family);
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.mobile-lang-dropdown {
  margin-top: 8px;
  display: none;
}

.mobile-lang-dropdown.active {
  display: block;
}

.mobile-lang-option {
  width: 100%;
  padding: 12px 16px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  text-align: left;
  font-size: 16px;
  font-family: var(--font-family);
  cursor: pointer;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.mobile-lang-option:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

/* ============================================
   HERO SECTION
   ============================================ */
.hero-section {
  min-height: 100vh;
  display: flex;
  align-items: flex-start;
  justify-content: center;
  padding: var(--header-offset-desktop) 24px 28px;
  text-align: center;
  position: relative;
  z-index: 2;
  margin-top: 0;
  margin-bottom: 0;
}

.hero-content {
  max-width: 800px;
  margin: 0 auto;
  padding-top: 0;
  margin-top: 0;
}

.hero-title {
  font-size: clamp(40px, 8vw, 72px);
  font-weight: 700;
  line-height: 1.1;
  letter-spacing: -1px;
  margin-top: 0;
  margin-bottom: 10px;
  color: var(--text-primary);
}

.hero-subtitle {
  font-size: clamp(18px, 2.5vw, 24px);
  color: var(--text-secondary);
  margin-bottom: 14px;
  line-height: 1.5;
  font-weight: 400;
}

.hero-trust-indicators {
  display: flex;
  flex-wrap: wrap;
  gap: 12px;
  justify-content: center;
  align-items: center;
  margin-bottom: 0;
}

.trust-badge {
  display: inline-flex;
  align-items: center;
  padding: 10px 20px;
  background: var(--bg-glass);
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  border: 1px solid var(--border-color);
  border-radius: 100px;
  font-size: 14px;
  font-weight: 500;
  color: var(--text-secondary);
  transition: all var(--transition-fast);
}

.trust-badge:hover {
  border-color: var(--border-color-hover);
  color: var(--text-primary);
  background: var(--bg-glass-strong);
}

/* Trust Block */
.trust-block {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 12px;
  margin-top: 12px;
  padding: 18px 20px;
  background: rgba(18, 18, 18, 0.5);
  backdrop-filter: blur(16px);
  -webkit-backdrop-filter: blur(16px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 20px;
  box-shadow: 
    0 4px 24px rgba(0, 0, 0, 0.4),
    0 2px 8px rgba(0, 0, 0, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.05);
}

.trust-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 8px;
  text-align: center;
  padding: 12px 8px;
}

.trust-item-icon {
  font-size: 24px;
  line-height: 1;
  opacity: 0.9;
}

.trust-item-text {
  font-size: 13px;
  color: rgba(255, 255, 255, 0.75);
  line-height: 1.4;
  font-weight: 400;
}

/* Reputation Block */
.reputation-block {
  max-width: 680px;
  margin: 16px auto 0;
  padding: 32px 40px;
  background: rgba(18, 18, 18, 0.45);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 20px;
  box-shadow: 
    0 2px 16px rgba(0, 0, 0, 0.3),
    0 1px 4px rgba(0, 0, 0, 0.2);
}

.reputation-title {
  font-size: 20px;
  font-weight: 600;
  color: rgba(255, 255, 255, 0.9);
  margin: 0 0 16px 0;
  line-height: 1.4;
}

.reputation-text {
  font-size: 15px;
  color: rgba(255, 255, 255, 0.75);
  line-height: 1.7;
  margin: 0 0 14px 0;
  font-weight: 400;
}

.reputation-text:last-child {
  margin-bottom: 0;
}

/* ============================================
   SERVICES SECTION
   ============================================ */
.services-section {
  padding: 32px 0 var(--section-pad-y-desktop) 0;
  position: relative;
  z-index: 2;
  scroll-margin-top: var(--header-offset-desktop);
  margin-top: 0;
}

.services-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: var(--block-gap);
  align-items: stretch;
}

.services-category-title {
  grid-column: 1 / -1;
  font-size: 32px;
  font-weight: 700;
  margin: 0 0 24px;
  color: var(--text-primary);
  letter-spacing: -0.5px;
}

.services-category-title:first-child {
  margin-top: 0;
}

.service-card {
  position: relative;
  background: rgba(18, 18, 18, 0.45);
  background-image: linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.03) 0%,
    rgba(255, 255, 255, 0.01) 50%,
    rgba(0, 0, 0, 0.02) 100%
  );
  backdrop-filter: blur(16px) saturate(150%);
  -webkit-backdrop-filter: blur(16px) saturate(150%);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 20px;
  padding: 28px;
  display: flex;
  flex-direction: column;
  gap: 20px;
  transition: all 200ms ease-out;
  box-shadow: 
    0 4px 24px rgba(0, 0, 0, 0.4),
    0 2px 8px rgba(0, 0, 0, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.05);
  overflow: hidden;
  height: 100%;
}

.service-card::before {
  content: '';
  position: absolute;
  inset: 0;
  background: radial-gradient(
    circle at 20% 20%,
    rgba(255, 255, 255, 0.08) 0%,
    transparent 50%
  ),
  linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.04) 0%,
    transparent 40%
  );
  opacity: 0.45;
  pointer-events: none;
  z-index: 0;
  border-radius: 20px;
}

.service-card::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 40%;
  background: linear-gradient(
    to top,
    rgba(0, 0, 0, 0.15) 0%,
    transparent 100%
  );
  pointer-events: none;
  z-index: 0;
  border-radius: 0 0 20px 20px;
}

.service-card > * {
  position: relative;
  z-index: 1;
}

.service-card:hover {
  border-color: rgba(255, 255, 255, 0.15);
  box-shadow: 
    0 8px 32px rgba(0, 0, 0, 0.5),
    0 4px 12px rgba(0, 0, 0, 0.4),
    inset 0 1px 0 rgba(255, 255, 255, 0.08);
  transform: translateY(-2px);
}

.service-card:hover::before {
  opacity: 0.55;
}

.service-card:focus-visible {
  outline: 2px solid rgba(255, 255, 255, 0.3);
  outline-offset: 2px;
}

.card-header h3 {
  font-size: 20px;
  font-weight: 600;
  color: var(--text-primary);
  margin-bottom: 10px;
  line-height: 1.3;
}

.card-description {
  font-size: 15px;
  color: var(--text-secondary);
  line-height: 1.5;
  margin-bottom: 0;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  min-height: 2.4em;
}

.card-price-wrapper {
  display: flex;
  align-items: baseline;
  gap: 8px;
  margin-top: 4px;
}

.price {
  font-size: 32px;
  font-weight: 700;
  color: var(--text-primary);
  letter-spacing: -0.5px;
}

.card-actions {
  display: flex;
  flex-direction: column;
  gap: 12px;
  margin-top: auto;
}

.btn {
  padding: 14px 24px;
  border-radius: var(--radius-md);
  font-size: 15px;
  font-weight: 500;
  font-family: var(--font-family);
  cursor: pointer;
  border: none;
  transition: all var(--transition-base);
  text-align: center;
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  height: 48px;
  min-height: 48px;
  line-height: 1.2;
}

.btn-order {
  background: var(--accent-red);
  color: white;
  box-shadow: 0 4px 12px rgba(255, 59, 48, 0.3);
}

.btn-order:hover {
  background: var(--accent-red-hover);
  box-shadow: 0 6px 16px rgba(255, 59, 48, 0.4);
  transform: translateY(-2px);
}

.btn-order:active {
  background: var(--accent-red-active);
  transform: translateY(0);
}

.btn-preview,
.btn-more {
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  color: var(--text-secondary);
  border: 1px solid rgba(255, 255, 255, 0.1);
  position: relative;
}

.btn-preview:hover,
.btn-more:hover {
  color: var(--text-primary);
  border-color: rgba(255, 255, 255, 0.2);
  background: rgba(255, 255, 255, 0.08);
}

.btn-preview:focus-visible,
.btn-more:focus-visible {
  outline: 2px solid rgba(255, 255, 255, 0.3);
  outline-offset: 2px;
}

.card-trust-note {
  display: flex;
  align-items: center;
  gap: 8px;
  padding: 12px;
  background: var(--bg-glass);
  border-radius: var(--radius-sm);
  font-size: 13px;
  color: var(--text-muted);
}

.trust-icon {
  font-size: 16px;
}

.card-details {
  padding-top: 14px;
  border-top: 1px solid var(--border-color);
  margin-top: 4px;
}

.card-details p {
  font-size: 14px;
  color: var(--text-secondary);
  line-height: 1.6;
}

/* ============================================
   MODALS
   ============================================ */
.modal {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 3000;
  opacity: 0;
  transition: opacity var(--transition-base);
}

.modal.active {
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 1;
}

.modal::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
}

.modal-content {
  position: relative;
  background: var(--bg-card);
  backdrop-filter: blur(30px);
  -webkit-backdrop-filter: blur(30px);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-xl);
  padding: 40px;
  max-width: 540px;
  width: 90%;
  max-height: 90vh;
  overflow-y: auto;
  box-shadow: var(--shadow-xl);
  animation: modalSlideIn var(--transition-slow) ease;
}

@keyframes modalSlideIn {
  from {
    opacity: 0;
    transform: translateY(20px) scale(0.95);
  }
  to {
    opacity: 1;
    transform: translateY(0) scale(1);
  }
}

.modal-content .close {
  position: absolute;
  top: 20px;
  right: 20px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  font-size: 32px;
  cursor: pointer;
  padding: 0;
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
  line-height: 1;
}

.modal-content .close:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.modal-content h2 {
  font-size: 28px;
  font-weight: 700;
  color: var(--text-primary);
  margin-bottom: 16px;
  letter-spacing: -0.5px;
}

.modal-price {
  font-size: 36px;
  font-weight: 700;
  color: var(--text-primary);
  margin-bottom: 32px;
  letter-spacing: -0.5px;
}

.modal-trust-info {
  padding: 20px;
  background: var(--bg-glass);
  border-radius: var(--radius-md);
  margin-bottom: 32px;
}

.trust-text {
  font-size: 14px;
  color: var(--text-secondary);
  line-height: 1.6;
  margin: 0;
}

.modal-description h3 {
  font-size: 18px;
  font-weight: 600;
  color: var(--text-primary);
  margin-bottom: 16px;
}

.modal-description ul {
  list-style: none;
  padding: 0;
  margin: 0 0 32px 0;
}

.modal-description li {
  padding: 12px 0 12px 24px;
  position: relative;
  font-size: 15px;
  color: var(--text-secondary);
  line-height: 1.6;
}

.modal-description li::before {
  content: '‚Ä¢';
  position: absolute;
  left: 0;
  color: var(--accent-red);
  font-weight: bold;
  font-size: 20px;
}

.modal-actions {
  display: flex;
  flex-direction: column;
  gap: 12px;
}

.btn-pay {
  background: var(--accent-red);
  color: white;
  box-shadow: 0 4px 12px rgba(255, 59, 48, 0.3);
}

.btn-pay:hover {
  background: var(--accent-red-hover);
  box-shadow: 0 6px 16px rgba(255, 59, 48, 0.4);
  transform: translateY(-2px);
}

.btn-contact {
  background: transparent;
  color: var(--text-secondary);
  border: 1px solid var(--border-color);
}

.btn-contact:hover {
  color: var(--text-primary);
  border-color: var(--border-color-hover);
  background: var(--bg-glass);
}

.btn-secondary {
  background: transparent;
  color: var(--text-secondary);
  border: 1px solid var(--border-color);
}

.btn-secondary:hover {
  color: var(--text-primary);
  border-color: var(--border-color-hover);
  background: var(--bg-glass);
}

.payment-success-modal {
  text-align: center;
}

.payment-success-icon {
  width: 64px;
  height: 64px;
  margin: 0 auto 24px;
  background: rgba(52, 199, 89, 0.2);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32px;
  color: #34c759;
  font-weight: bold;
}

.payment-success-text {
  font-size: 16px;
  color: var(--text-secondary);
  margin-bottom: 32px;
}

/* ============================================
   FAQ SECTION
   ============================================ */
.faq-section {
  padding: var(--section-pad-y-desktop) 0;
  position: relative;
  z-index: 2;
  border: none !important;
  border-top: none !important;
  border-bottom: none !important;
  box-shadow: none !important;
  scroll-margin-top: var(--header-offset-desktop);
}

.faq-section::before,
.faq-section::after {
  content: none !important;
  display: none !important;
}

.section-title {
  font-size: 40px;
  font-weight: 700;
  color: var(--text-primary);
  text-align: center;
  margin-bottom: 32px;
  letter-spacing: -0.5px;
}

.faq-container {
  max-width: 800px;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  gap: 14px;
  border: none !important;
  box-shadow: none !important;
}

.faq-item {
  position: relative;
  background: rgba(18, 18, 18, 0.45);
  background-image: linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.03) 0%,
    rgba(255, 255, 255, 0.01) 50%,
    rgba(0, 0, 0, 0.02) 100%
  );
  backdrop-filter: blur(16px) saturate(150%);
  -webkit-backdrop-filter: blur(16px) saturate(150%);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 20px;
  overflow: hidden;
  transition: all 200ms ease-out;
  box-shadow: 
    0 4px 24px rgba(0, 0, 0, 0.4),
    0 2px 8px rgba(0, 0, 0, 0.3),
    inset 0 1px 0 rgba(255, 255, 255, 0.05);
}

.faq-item::before {
  content: '';
  position: absolute;
  inset: 0;
  background: radial-gradient(
    circle at 20% 20%,
    rgba(255, 255, 255, 0.08) 0%,
    transparent 50%
  ),
  linear-gradient(
    135deg,
    rgba(255, 255, 255, 0.04) 0%,
    transparent 40%
  );
  opacity: 0.45;
  pointer-events: none;
  z-index: 0;
  border-radius: 20px;
}

.faq-item > * {
  position: relative;
  z-index: 1;
}

.faq-item:hover {
  border-color: rgba(255, 255, 255, 0.15);
  box-shadow: 
    0 8px 32px rgba(0, 0, 0, 0.5),
    0 4px 12px rgba(0, 0, 0, 0.4),
    inset 0 1px 0 rgba(255, 255, 255, 0.08);
}

.faq-item:hover::before {
  opacity: 0.55;
}

.faq-question {
  padding: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  cursor: pointer;
  user-select: none;
}

.faq-question h3 {
  font-size: 18px;
  font-weight: 600;
  color: var(--text-primary);
  margin: 0;
  flex: 1;
}

.faq-icon {
  font-size: 24px;
  color: var(--text-secondary);
  transition: transform var(--transition-base);
  font-weight: 300;
  width: 24px;
  text-align: center;
}

.faq-item.active .faq-icon {
  transform: rotate(45deg);
}

.faq-answer {
  max-height: 0;
  overflow: hidden;
  transition: max-height var(--transition-slow) ease, padding var(--transition-slow) ease;
  padding: 0 24px;
}

.faq-item.active .faq-answer {
  max-height: 500px;
  padding: 0 20px 20px;
}

.faq-answer p {
  font-size: 15px;
  color: var(--text-secondary);
  line-height: 1.6;
  margin: 0;
}

/* ============================================
   FOOTER
   ============================================ */
.footer {
  padding: 48px 0 24px;
  border-top: 1px solid var(--border-color);
  margin-top: var(--section-pad-y-desktop);
  position: relative;
  z-index: 2;
  box-shadow: none !important;
}

.footer::before,
.footer::after {
  content: none !important;
  display: none !important;
}

.footer-info {
  text-align: center;
  margin-bottom: 36px;
}

.footer-info p {
  font-size: 15px;
  color: var(--text-secondary);
  line-height: 1.6;
  margin-bottom: 16px;
}

.footer-trust {
  font-size: 14px;
  color: var(--text-muted);
  line-height: 1.6;
}

.footer-links {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  gap: 24px;
}

.footer-link {
  color: var(--text-secondary);
  text-decoration: none;
  font-size: 15px;
  font-weight: 500;
  padding: 8px 16px;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
}

.footer-link:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

/* ============================================
   ADMIN PANEL
   ============================================ */
.admin-login-modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 4000;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, 0.8);
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
}

.admin-login-content {
  background: var(--bg-card);
  backdrop-filter: blur(30px);
  -webkit-backdrop-filter: blur(30px);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-xl);
  padding: 40px;
  max-width: 400px;
  width: 90%;
  box-shadow: var(--shadow-xl);
}

.admin-close-btn {
  position: absolute;
  top: 20px;
  right: 20px;
  background: transparent;
  border: none;
  color: var(--text-secondary);
  font-size: 32px;
  cursor: pointer;
  padding: 0;
  width: 32px;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: var(--radius-sm);
  transition: all var(--transition-fast);
  line-height: 1;
}

.admin-close-btn:hover {
  color: var(--text-primary);
  background: var(--bg-glass);
}

.admin-login-title {
  font-size: 28px;
  font-weight: 700;
  color: var(--text-primary);
  margin-bottom: 8px;
  text-align: center;
}

.admin-login-subtitle {
  font-size: 15px;
  color: var(--text-secondary);
  margin-bottom: 32px;
  text-align: center;
}

.admin-login-form {
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.admin-password-input {
  padding: 14px 16px;
  background: var(--bg-glass);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-md);
  color: var(--text-primary);
  font-size: 15px;
  font-family: var(--font-family);
  transition: all var(--transition-fast);
}

.admin-password-input:focus {
  outline: none;
  border-color: var(--border-color-hover);
  background: var(--bg-glass-strong);
}

.admin-login-button {
  padding: 14px 24px;
  background: var(--accent-red);
  color: white;
  border: none;
  border-radius: var(--radius-md);
  font-size: 15px;
  font-weight: 500;
  font-family: var(--font-family);
  cursor: pointer;
  transition: all var(--transition-base);
}

.admin-login-button:hover {
  background: var(--accent-red-hover);
  transform: translateY(-2px);
}

.admin-error-message {
  color: var(--accent-red);
  font-size: 14px;
  text-align: center;
  padding: 8px;
}

.admin-panel {
  position: fixed;
  bottom: 24px;
  right: 24px;
  background: var(--bg-card);
  backdrop-filter: blur(30px);
  -webkit-backdrop-filter: blur(30px);
  border: 1px solid var(--border-color);
  border-radius: var(--radius-xl);
  box-shadow: var(--shadow-xl);
  max-width: 400px;
  width: 90%;
  z-index: 3500;
}

.admin-panel-header {
  padding: 20px;
  border-bottom: 1px solid var(--border-color);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.admin-panel-title {
  font-size: 20px;
  font-weight: 600;
  color: var(--text-primary);
  margin: 0;
}

.admin-panel-actions {
  display: flex;
  gap: 8px;
}

.admin-action-button {
  padding: 8px 16px;
  background: transparent;
  border: 1px solid var(--border-color);
  border-radius: var(--radius-sm);
  color: var(--text-secondary);
  font-size: 14px;
  font-family: var(--font-family);
  cursor: pointer;
  transition: all var(--transition-fast);
}

.admin-action-button:hover {
  color: var(--text-primary);
  border-color: var(--border-color-hover);
  background: var(--bg-glass);
}

.admin-panel-content {
  padding: 20px;
}

.admin-loading {
  text-align: center;
  padding: 40px 20px;
}

.admin-spinner {
  width: 32px;
  height: 32px;
  border: 3px solid var(--border-color);
  border-top-color: var(--accent-red);
  border-radius: 50%;
  animation: spin 1s linear infinite;
  margin: 0 auto 16px;
}

@keyframes spin {
  to {
    transform: rotate(360deg);
  }
}

/* ============================================
   RESPONSIVE DESIGN
   ============================================ */
@media (max-width: 1024px) {
  .services-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 18px;
  }
}

@media (max-width: 768px) {
  .nav-desktop {
    display: none;
  }

  .lang-desktop {
    display: none;
  }

  .mobile-menu-toggle {
    display: block;
  }

  .mobile-menu {
    display: block;
  }

  .services-grid {
    grid-template-columns: 1fr;
    gap: 16px;
  }

  .hero-section {
    padding: var(--header-offset-tablet) 24px 28px;
    margin-top: 0;
    margin-bottom: 0;
    align-items: flex-start;
  }

  .hero-title {
    font-size: 36px;
    margin-bottom: 10px;
  }

  .hero-subtitle {
    font-size: 18px;
    margin-bottom: 14px;
  }

  .trust-block {
    grid-template-columns: repeat(2, 1fr);
    gap: 10px;
    padding: 16px 16px;
    margin-top: 14px;
  }

  .trust-item {
    padding: 10px 8px;
  }

  .trust-item-icon {
    font-size: 22px;
  }

  .trust-item-text {
    font-size: 12px;
  }

  .reputation-block {
    max-width: 100%;
    margin: 14px 0 0;
    padding: 28px 32px;
  }

  .reputation-title {
    font-size: 19px;
    margin-bottom: 14px;
  }

  .reputation-text {
    font-size: 14px;
    line-height: 1.65;
    margin-bottom: 12px;
  }

  .services-section {
    padding: 28px 0 var(--section-pad-y-tablet) 0;
    margin-top: 0;
    scroll-margin-top: var(--header-offset-tablet);
  }

  .faq-section {
    padding: var(--section-pad-y-tablet) 0;
  }

  .services-category-title {
    font-size: 28px;
    margin: 0 0 20px;
  }

  .section-title {
    font-size: 32px;
    margin-bottom: 28px;
  }

  .footer {
    margin-top: var(--section-pad-y-tablet);
    padding: 40px 0 20px;
  }

  .footer-info {
    margin-bottom: 32px;
  }

  .service-card {
    backdrop-filter: blur(12px) saturate(140%);
    -webkit-backdrop-filter: blur(12px) saturate(140%);
  }

  .faq-item {
    backdrop-filter: blur(12px) saturate(140%);
    -webkit-backdrop-filter: blur(12px) saturate(140%);
  }

  .modal-content {
    padding: 32px 24px;
  }

  .admin-panel {
    bottom: 16px;
    right: 16px;
    left: 16px;
    max-width: none;
  }
}

@media (max-width: 480px) {
  .container {
    padding: 0 16px;
  }

  .nav-wrapper {
    padding: 12px 16px;
  }

  .hero-section {
    padding: var(--header-offset-mobile) 16px 24px;
    margin-top: 0;
    margin-bottom: 0;
    align-items: flex-start;
  }

  .hero-title {
    margin-bottom: 10px;
  }

  .hero-subtitle {
    margin-bottom: 14px;
  }

  .services-section {
    padding: 24px 0 var(--section-pad-y-mobile) 0;
    margin-top: 0;
    scroll-margin-top: var(--header-offset-mobile);
  }

  .faq-section {
    padding: var(--section-pad-y-mobile) 0;
    scroll-margin-top: var(--header-offset-mobile);
  }

  .services-category-title {
    margin: 0 0 18px;
  }

  .section-title {
    margin-bottom: 24px;
  }

  .faq-container {
    gap: 12px;
  }

  .faq-question {
    padding: 18px;
  }

  .faq-item.active .faq-answer {
    padding: 0 18px 18px;
  }

  .service-card {
    padding: 24px;
    backdrop-filter: blur(10px) saturate(130%);
    -webkit-backdrop-filter: blur(10px) saturate(130%);
  }

  .faq-item {
    backdrop-filter: blur(10px) saturate(130%);
    -webkit-backdrop-filter: blur(10px) saturate(130%);
  }

  .footer {
    margin-top: var(--section-pad-y-mobile);
    padding: 36px 0 20px;
  }

  .footer-info {
    margin-bottom: 28px;
  }

  .trust-badge {
    font-size: 13px;
    padding: 8px 16px;
  }

  .trust-block {
    grid-template-columns: repeat(2, 1fr);
    gap: 10px;
    padding: 16px 12px;
    margin-top: 12px;
  }

  .trust-item {
    padding: 10px 6px;
    gap: 6px;
  }

  .trust-item-icon {
    font-size: 20px;
  }

  .trust-item-text {
    font-size: 12px;
  }

  .reputation-block {
    max-width: 100%;
    margin: 12px 0 0;
    padding: 24px 20px;
  }

  .reputation-title {
    font-size: 18px;
    margin-bottom: 12px;
  }

  .reputation-text {
    font-size: 14px;
    line-height: 1.65;
    margin-bottom: 12px;
  }
}
