/* ============================================
   –í–ò–î–ï–û–§–û–ù (Video Background)
   ============================================ */
.video-bg-layer {
  position: fixed;
  inset: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
  pointer-events: none;
  overflow: hidden;
}

.video-bg {
  position: absolute;
  top: 50%;
  left: 50%;
  min-width: 100%;
  min-height: 100%;
  width: auto;
  height: auto;
  transform: translate(-50%, -50%);
  object-fit: cover;
  pointer-events: none;
  will-change: transform;
}

.video-overlay {
  position: fixed;
  inset: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background: rgba(0, 0, 0, 0.4);
  background-image: 
    radial-gradient(circle at 20% 30%, rgba(139, 0, 0, 0.12) 0%, transparent 50%),
    radial-gradient(circle at 80% 70%, rgba(139, 0, 0, 0.1) 0%, transparent 50%),
    radial-gradient(circle at 50% 50%, rgba(139, 0, 0, 0.06) 0%, transparent 70%),
    linear-gradient(to bottom, rgba(0, 0, 0, 0.15) 0%, transparent 20%),
    linear-gradient(to top, rgba(0, 0, 0, 0.15) 0%, transparent 20%);
  pointer-events: none;
}

/* Fallback: –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ –≤–∏–¥–µ–æ –Ω–∞ –º–æ–±–∏–ª—å–Ω—ã—Ö –∏ —Å–ª–∞–±—ã—Ö —É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞—Ö */
@media (max-width: 768px) {
  .video-bg-layer,
  .video-bg {
    display: none;
  }
  
  .video-overlay {
    display: none;
  }
}

/* –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –≤–∏–¥–µ–æ –ø—Ä–∏ prefers-reduced-motion */
@media (prefers-reduced-motion: reduce) {
  .video-bg-layer,
  .video-bg {
    display: none;
  }
  
  .video-overlay {
    display: none;
  }
}

/* –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –≤–∏–¥–µ–æ –Ω–∞ –æ—á–µ–Ω—å –º–∞–ª–µ–Ω—å–∫–∏—Ö —ç–∫—Ä–∞–Ω–∞—Ö */
